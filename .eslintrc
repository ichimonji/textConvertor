{
    root: true,
    parser: 'babel-eslint',
    parserOptions: {
      sourceType: 'module'
    },
    extends: [
      'airbnb-base'
    ],
    plugins: [
      'html'
    ],
    env: {
      'browser': true
    },
    rules: {
      'arrow-parens': 0,
      'no-new': 0,
      'import/prefer-default-export': 0,
      'import/no-unresolved': 0,
      'no-undef': 0,
      'no-param-reassign': 0,
      'indent': [2, 2, { "SwitchCase": 1 }],
      'linebreak-style': 0,
      'one-var': ['error', 'consecutive'],
      'func-names': 0,
      'no-extend-native': 0,
      'comma-dangle': 0,
      'no-loop-func': 0,
      'no-irregular-whitespace': ['error', { 'skipRegExps': true }]
    }
}
